{
  "name": "driva-test-full-stack-engineer-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "build-dev": "tsc --w",
    "clean": "rm -rf dist",
    "start": "npm run clean && npm run build && node -r source-map-support/register dist/index.js",
    "start-dev-inspect": "tsc && nodemon --inspect ./dist/index.js",
    "start-dev": "nodemon ./dist/index.js",
    "test": "jest --watch",
    "test-ci": "jest --coverage -u"
  },
  "dependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.6",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "mysql2": "^2.0.1",
    "nodemon": "^2.0.1",
    "sequelize": "^6.13.0",
    "sequelize-cli": "^5.5.1",
    "sequelize-typescript": "^2.1.2",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/jest": "^24.0.23",
    "@types/node": "^15.3.1",
    "jest": "^24.9.0",
    "ts-jest": "^24.2.0",
    "ts-node": "^9.1.1",
    "tslint": "^5.12.0",
    "tslint-config-shopify": "^3.0.2",
    "typescript": "^4.2.4"
  }
}
